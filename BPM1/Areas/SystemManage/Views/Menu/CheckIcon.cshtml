<script src="/miniui/jquery-1.8.2.min.js"></script>
<link type="text/css" href="/page/iconfont/iconfont.css" rel="stylesheet" />

<style type="text/css">
    div.name, div.fontclass {
        display: none;
    }
    ul > li {
        display:block;
        float:left;
        text-decoration:none;
        /*padding:5px;*/
        cursor:pointer;
        height:48px;
        width:48px;
    }
    li > i {
        font-size:30px;
    }
    .hoverClass {
        font-size:32px;
    }

</style>

<script type="text/javascript">
    var className = "";
    $(function () {

        $("i").css({"font-size":"30px"});


        $("i").mouseover(function () {
            $(this).addClass("hoverClass");
        }).mouseout(function () {
                $(this).removeClass("hoverClass");
            }).click(function () {
                var html = $(this).parent().find("div.fontclass").text();
                className = html.replace(".", "");
            }).dblclick(function () { 
                var html = $(this).parent().find("div.fontclass").text();
                className = html.replace(".", "");
                onCancel();
            });
    })

    function GetData() {
        return className;
    }



    function CloseWindow(action) {
        if (window.CloseOwnerWindow) return window.CloseOwnerWindow(action);
        else window.close();
    }

    function onOk() {
        CloseWindow("ok");
    }
    function onCancel() {
        CloseWindow("cancel");
    }

</script>



